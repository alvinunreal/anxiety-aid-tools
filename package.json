{
  "name": "nuxt-boilerplate",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev-https": "nuxt dev --https --ssl-cert certs/localhost.pem --ssl-key certs/localhost-key.pem",
    "generate": "nuxt generate",
    "preview-https": "NITRO_SSL_CERT=`cat certs/localhost.pem` NITRO_SSL_KEY=`cat certs/localhost-key.pem` node .output/server/index.mjs",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext .ts,.js,.vue .",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write . --fix",
    "sync-locales": "node scripts/sync-locales.js"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@iconify-json/material-symbols": "^1.2.33",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/ph": "^1.2.2",
    "@intlify/eslint-plugin-vue-i18n": "^4.1.0",
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/icon": "^1.15.0",
    "@nuxt/image": "1.10.0",
    "@nuxtjs/device": "^3.2.4",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.11.2",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.18.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "nuxt": "^3.18.1",
    "postcss-import": "^16.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.4",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/i18n": "^10.0.6",
    "@nuxtjs/seo": "^3.1.0",
    "@sentry/nuxt": "^10.8.0",
    "@vite-pwa/nuxt": "^1.0.4",
    "gsap": "^3.13.0",
    "import-in-the-middle": "^1.14.2",
    "three": "^0.179.1"
  },
  "pnpm": {
    "overrides": {
      "@vercel/nft": "^0.27.4"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@sentry/cli",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}